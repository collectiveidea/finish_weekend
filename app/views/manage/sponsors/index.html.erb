<%= render 'manage/navigation' %>

<%= link_to "Add Sponsor", new_manage_event_sponsor_path(@event), class: 'btn' %>

<ul id="sponsor-list">
  <% @event.sponsors.each do |sponsor| %>
    <li>
    <div class="btn-group pull-right">
      <%= link_to "Edit", edit_manage_event_sponsor_path(@event, sponsor), class: 'btn' %>
      <%= link_to "Delete", manage_event_sponsor_path(@event, sponsor), method: :delete, class: 'btn btn-danger' %>
    </div>
    <%= sponsor.name %>
    </li>
  <% end %>
</ul>

